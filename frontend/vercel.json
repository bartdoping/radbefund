{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "functions": {},
  "regions": ["fra1"],
  "framework": null,
  "buildCommand": "echo 'Static HTML - no build required'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install required'",
  "devCommand": "echo 'No dev required'"
}